{
  "name": "backend-app",
  "version": "1.0.0",
  "description": "Backend Application",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "build": "npm-run-all clean transpile cpy:assets",
    "clean": "rimraf dist",
    "transpile": "npx babel ./src -d dist",
    "cpy:assets": "cp -R ./src/assets ./dist/assets",
    "server": "babel-node ./dist/index.js",
    "start:dev:old": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "test": "jest --detectOpenHandles",
    "test2": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start:dev:server": "babel-node ./src/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec npm run start:dev:server"
  },
  "author": "Robert Kerscher",
  "license": "",
  "dependencies": {
    "@azure/storage-blob": "^12.8.0",
    "@paypal/checkout-server-sdk": "^1.0.2",
    "@sinonjs/fake-timers": "^7.1.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "latest",
    "cors": "latest",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "latest",
    "express-basic-auth": "^1.2.1",
    "express-validation": "^3.0.8",
    "helmet": "^3.23.3",
    "http-status-codes": "^2.1.4",
    "into-stream": "^7.0.0",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.29.1",
    "mongod": "^2.0.0",
    "mongodb": "^3.6.6",
    "multer": "^1.4.2",
    "nanoid": "^3.1.25",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "qs": "^6.10.1",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.6.1",
    "sequelize": "^6.6.2",
    "sharp": "^0.28.1",
    "stream": "^0.0.2",
    "streamifier": "^0.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "moxios": "latest",
    "nodemon": "^1.19.4",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/api-requests/"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run start:dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/tests/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
